{
  "name": "DevQuest",
  "version": "1.0.0",
  "private": true,
  "description": "Unified dependencies for client and server",
  "scripts": {
    "dev:client": "cd client && npm start",
    "dev:server": "cd server && nodemon server.js",
    "dev:both": "concurrently \"cd server && nodemon server.js\" \"npm run dev:client\"",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build": "cd client && npm run build",
    "start:client": "cd client && npm start",
    "start:server": "cd server && node server.js",
    "start": "npm run start:client & npm run start:server"
  },
