{
  "name": "DevQuest",
  "version": "1.0.0",
  "private": true,
  "description": "Unified dependencies for client and server",
  "main": "index.js",
  "scripts": {
    "dev:client": "cd client && vite",
    "dev:server": "cd server && nodemon server.js",
    "dev:both": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build:client": "cd client && vite build",
    "build:server": "cd server && npm run build",
    "build": "npm run build:client && npm run build:server",
    "start:client": "cd client && vite preview",
    "start:server": "cd server && node server.js",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
